{
  "name": "@txnlab/use-wallet-ui-react",
  "version": "0.2.0",
  "description": "React components for use-wallet UI",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./dist/style.css": "./dist/style.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "algorand",
    "wallet",
    "use-wallet",
    "react",
    "vue",
    "solid",
    "ui"
  ],
  "author": "TxnLab Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Algorand-Developer-Retreat/use-wallet-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Algorand-Developer-Retreat/use-wallet-ui/issues"
  },
  "homepage": "https://github.com/Algorand-Developer-Retreat/use-wallet-ui#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "scripts": {
    "build": "pnpm generate:css && vite build && publint --strict",
    "dev": "vite build --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "generate:css": "npx @tailwindcss/cli -c ./tailwind.config.js -i ./src/input.css -o ./dist/style.css"
  },
  "peerDependencies": {
    "@txnlab/use-wallet-react": "^4.0.1",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.5",
    "@headlessui/react": "^2.2.0",
    "@tanstack/react-query": "^5.72.2",
    "@txnlab/utils-ts": "^0.1.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.3",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "algosdk": "^3.2.0",
    "publint": "^0.3.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.0.17",
    "tsx": "^4.19.3",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
